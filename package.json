{
  "name": "daheim-app-ui",
  "version": "0.1.0",
  "description": "Daheim App UI",
  "scripts": {
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "check": "eslint .",
    "build-babel": "babel src/ -d lib/",
    "build-webpack": "NODE_ENV=production webpack --config webpack/production.config.js",
    "build": "npm run build-babel && npm run build-webpack",
    "clean": "rm -rf lib && rm -rf build",
    "start": "NODE_ENV=production node ./lib/start"
  },
  "author": "Gergo Ertli",
  "license": "ISC",
  "betterScripts": {
    "start-dev": {
      "command": "babel-watch ./src/start",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "watch-client": {
      "command": "webpack-dev-server --config webpack/development.config.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    }
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.7.4",
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-watch": "^2.0.2",
    "better-npm-run": "0.0.8",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.6.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.3",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "bluebird": "^3.3.5",
    "classnames": "^2.2.3",
    "cookie-parser": "^1.4.1",
    "es6-error": "^2.1.0",
    "eslint-plugin-react": "^5.0.1",
    "exports-loader": "^0.6.3",
    "express": "^4.13.4",
    "flux-standard-action": "^0.6.1",
    "http-proxy": "^1.13.2",
    "imports-loader": "^0.6.5",
    "lodash": "^4.7.0",
    "material-ui": "^0.15.0",
    "md5": "^2.1.0",
    "moment": "^2.12.0",
    "node-uuid": "^1.4.7",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-dropzone": "^3.4.0",
    "react-helmet": "^3.0.1",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "react-star-rating": "^1.4.2",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-thunk": "^2.0.1",
    "screenfull": "^3.0.0",
    "socket.io-client": "^1.4.5",
    "webpack-isomorphic-tools": "^2.2.48",
    "webpack-merge": "^0.11.0",
    "webrtc-adapter-test": "^0.2.10",
    "whatwg-fetch": "^0.11.0"
  }
}
