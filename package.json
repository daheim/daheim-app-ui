{
  "name": "daheim-app-ui",
  "version": "0.1.0",
  "description": "Daheim App UI",
  "scripts": {
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "check": "eslint .",
    "build-babel": "babel src/ -d lib/",
    "build-webpack": "NODE_ENV=production webpack --config webpack/production.config.js",
    "build": "npm run build-babel && npm run build-webpack",
    "clean": "rm -rf lib && rm -rf build",
    "start": "NODE_ENV=production node ./lib/start",
    "test": "jest --no-cache",
    "cover": "jest --no-cache --coverage && open tmp/coverage/lcov-report/index.html"
  },
  "author": "Gergo Ertli",
  "license": "MIT",
  "betterScripts": {
    "start-dev": {
      "command": "babel-watch ./src/start",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "watch-client": {
      "command": "webpack-dev-server --config webpack/development.config.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/egergo/daheim-app-ui.git"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "coverageDirectory": "<rootDir>/tmp/coverage",
    "testPathDirs": [
      "<rootDir>/src",
      "<rootDir>/__mocks__"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "core.js",
      "babel-polyfill",
      "babel-runtime",
      "crypto",
      "redux-actions",
      "flux-standard-action",
      "lodash",
      "sinon",
      "chai",
      "chai-as-promised"
    ]
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.2",
    "babel-jest": "^12.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-watch": "^2.0.2",
    "better-npm-run": "0.0.8",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-plugin-standard": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.3",
    "jest-cli": "^12.0.2",
    "json-loader": "^0.5.4",
    "node-sass": "^3.7.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-merge": "^0.12.0"
  },
  "dependencies": {
    "bluebird": "^3.3.5",
    "classnames": "^2.2.3",
    "cookie": "^0.2.3",
    "cookie-parser": "^1.4.1",
    "debug": "^2.2.0",
    "es6-error": "^2.1.1",
    "eslint-plugin-react": "^5.0.1",
    "exports-loader": "^0.6.3",
    "express": "^4.13.4",
    "flux-standard-action": "^0.6.1",
    "http-proxy": "^1.13.2",
    "imports-loader": "^0.6.5",
    "lodash": "^4.11.2",
    "material-ui": "^0.15.0",
    "md5": "^2.1.0",
    "moment": "^2.12.0",
    "node-uuid": "^1.4.7",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-dropzone": "^3.4.0",
    "react-helmet": "^3.0.1",
    "react-redux": "^4.4.1",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-thunk": "^2.0.1",
    "request-promise": "^3.0.0",
    "scroll-behavior": "^0.7.0",
    "serialize-javascript": "^1.2.0",
    "socket.io-client": "^1.4.6",
    "spdy": "^3.3.2",
    "webpack-isomorphic-tools": "^2.2.50",
    "webrtc-adapter": "^1.1.0",
    "whatwg-fetch": "^1.0.0"
  }
}
